{
    "manifest_version": 2,

    "name": "Facebook Ad Killer",
    "description": "Finally remove sponsored posts.",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "index.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": ["alarms"]
  }
